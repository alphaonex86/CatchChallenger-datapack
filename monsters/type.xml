<!--
multiplicator number need be multiple of 2: 2, 0.5, 0
All not into multiplicator is not multiplied or divided
The to value is separated by ;
color is optional



Fly > fight > stone > fly
-->

<types>
    <type name="normal" color="#B5B5B5">
        <name>Normal</name>
        <name lang="fr">Normal</name>
        <multiplicator number="0.5" to="stone;steal"/>
        <multiplicator number="0" to="spectre"/>
    </type>
    <type name="electric" color="#FFEC10">
        <name>Electric</name>
        <name lang="fr">Electrique</name>
        <multiplicator number="0.5" to="fire;plant;electric"/>
        <multiplicator number="2" to="bird;water"/>
        <multiplicator number="0" to="ground"/>
    </type>
    <type name="bird" color="#8A47C5">
        <name>Bird</name>
        <name lang="fr">Oiseau</name>
        <multiplicator number="2" to="plant;fight"/>
        <multiplicator number="0.5" to="steal;electric;stone;bird"/>
    </type>
    <type name="fire" color="#f38233">
        <name>Fire</name>
        <name lang="fr">Feu</name>
        <multiplicator number="0.5" to="water;fire;stone;steal"/>
        <multiplicator number="2" to="plant;ice"/>
    </type>
    <type name="water" color="#2E9EF3">
        <name>Water</name>
        <name lang="fr">Eau</name>
        <multiplicator number="0.5" to="plant;water"/>
        <multiplicator number="2" to="fire;stone;ground"/>
    </type>
    <type name="stone" color="#BF904E">
        <name>Stone</name>
        <name lang="fr">Pierre</name>
        <multiplicator number="0.5" to="ground;fight;stone;steal;plant"/>
        <multiplicator number="2" to="bird;fire;ice"/>
    </type>
    <type name="ground" color="#926E3B">
        <name>Ground</name>
        <name lang="fr">Sol</name>
        <multiplicator number="2" to="fire;electric;poison;stone;steal"/>
        <multiplicator number="0" to="plant"/>
    </type>
    <type name="plant" color="#71DA5E">
        <name>Plant</name>
        <name lang="fr">Plante</name>
        <multiplicator number="0.5" to="fire;plant;steal;poison;bird"/>
        <multiplicator number="2" to="water;ground;stone"/>
    </type>
    <type name="ice" color="#75E7F3">
        <name>Ice</name>
        <name lang="fr">Glace</name>
        <multiplicator number="0.5" to="fire;water;steal;ice"/>
        <multiplicator number="2" to="plant;ground;bird"/>
    </type>
    <type name="fight" color="#F34D4D">
        <name>Fight</name>
        <name lang="fr">Combat</name>
        <multiplicator number="0.5" to="poison;bird;psy"/>
        <multiplicator number="2" to="normal;ice;stone;steal"/>
        <multiplicator number="0" to="spectre"/>
    </type>
    <type name="poison" color="#B46DC5">
        <name>Poison</name>
        <name lang="fr">Poison</name>
        <multiplicator number="0.5" to="poison;ground;stone;spectre"/>
        <multiplicator number="2" to="plant;fight"/>
        <multiplicator number="0" to="steal"/>
    </type>
    <type name="psy" color="#498E3D">
        <name>Psy</name>
        <name lang="fr">Psy</name>
        <multiplicator number="0.5" to="psy;steal"/>
        <multiplicator number="2" to="poison;fight"/>
    </type>
    <type name="spectre" color="#CBFF0F">
        <name>Spectre</name>
        <name lang="fr">Spectre</name>
        <multiplicator number="2" to="psy;spectre"/>
        <multiplicator number="0.5" to="steal"/>
        <multiplicator number="0" to="normal"/>
    </type>
    <type name="steal" color="#747579">
        <name>Steal</name>
        <name lang="fr">Metal</name>
        <multiplicator number="2" to="ice;stone"/>
        <multiplicator number="0.5" to="water;fire;steal"/>
    </type>
</types>
